<!DOCTYPE html>
<html class="no-js" lang="zxx">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--=== Favicon ===-->
    <link rel="shortcut icon" href="favicon.ico" type="image/x-icon" />

    <title>Pembayaran</title>

    <!--=== Bootstrap CSS ===-->
    <link href="/static/assets_user/css/bootstrap.min.css" rel="stylesheet">
    <!--=== Vegas Min CSS ===-->
    <link href="/static/assets_user/css/plugins/vegas.min.css" rel="stylesheet">
    <!--=== Slicknav CSS ===-->
    <link href="/static/assets_user/css/plugins/slicknav.min.css" rel="stylesheet">
    <!--=== Magnific Popup CSS ===-->
    <link href="/static/assets_user/css/plugins/magnific-popup.css" rel="stylesheet">
    <!--=== Owl Carousel CSS ===-->
    <link href="/static/assets_user/css/plugins/owl.carousel.min.css" rel="stylesheet">
    <!--=== Gijgo CSS ===-->
    <link href="/static/assets_user/css/plugins/gijgo.css" rel="stylesheet">
    <!--=== FontAwesome CSS ===-->
    <link href="/static/assets_user/css/font-awesome.css" rel="stylesheet">
    <!--=== Theme Reset CSS ===-->
    <link href="/static/assets_user/css/reset.css" rel="stylesheet">
    <!--=== Main Style CSS ===-->
    <link href="/static/assets_user/css/style.css" rel="stylesheet">
    <!--=== Responsive CSS ===-->
    <link href="/static/assets_user/css/responsive.css" rel="stylesheet">
</head>

<body class="loader-active">
    <!--== Header Area Start ==-->
    <header id="header-area" class="fixed-top">

        <!--== Header Bottom Start ==-->
        <div id="header-bottom">
            <div class="container">
                <div class="row">
                    <!--== Logo Start ==-->
                    <div class="col-lg-4">
                        
                            <h3>Sistem Perumahan</h3>
                        
                    </div>
                    <!--== Logo End ==-->

                    <!--== Main Menu Start ==-->
                    <div class="col-lg-8 d-none d-xl-block">
                        <nav class="mainmenu alignright">
                            <% if (user) { %>
                                <ul>
                                    <li><a href="/dashboard">Dashboard</a></li>
                                    <li><a href="/profil">Profil</a></li>
                                    <li><a href="/pembayarandetail">Pembayaran</a></li>
                                    <li><a href="/logout">Welcome <%= user.name %> <span> | Logout</span></a></li>
                                </ul>
                            <% } %>
                        </nav>
                    </div>
                    <!--== Main Menu End ==-->
                </div>
            </div>
        </div>
        <!--== Header Bottom End ==-->
    </header>
    <!--== Header Area End ==-->

    <div class="container">
        <div class="card" style="margin-top: 150px; margin-bottom: 50px">
            <div class="card-header">
                Form Update Profil
            </div>
            <div class="card-body">
                <form action="/users1/edit/" method="POST">
                    <input type="hidden" name="id" class="form-control" value="<%= user.id %>">
                    <div class="form-group">
                        <label for="">Nama</label>
                        <input type="text" name="name" class="form-control" value="<%= user.name %>">
                    </div>
                    <div class="form-group">
                        <label for="">Username</label>
                        <input type="text" name="username" class="form-control" value="<%= user.username %>">
                    </div>
                    <div class="form-group">
                        <label for="">Password</label>
                        <input type="password" name="password" class="form-control" value="<%= user.password %>">
                    </div>
                    <button type="submit" class="btn btn-primary">Submit</button>
                </form>
            </div>
        </div>
    </div>

    <!--== Footer Area Start ==-->
    <section id="footer-area">

    <!-- Footer Bottom Start -->
    <div class="footer-bottom-area fixed-bottom">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                </div>
            </div>
        </div>
    </div>
    <!-- Footer Bottom End -->
</section>
<!--== Footer Area End ==-->
