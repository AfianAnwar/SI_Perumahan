<!DOCTYPE html>
<html class="no-js" lang="zxx">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--=== Favicon ===-->
    <link rel="shortcut icon" href="favicon.ico" type="image/x-icon" />

    <title>Dashboard</title>

    <!--=== Bootstrap CSS ===-->
    <link href="/static/assets_user/css/bootstrap.min.css" rel="stylesheet">
    <!--=== Vegas Min CSS ===-->
    <link href="/static/assets_user/css/plugins/vegas.min.css" rel="stylesheet">
    <!--=== Slicknav CSS ===-->
    <link href="/static/assets_user/css/plugins/slicknav.min.css" rel="stylesheet">
    <!--=== Magnific Popup CSS ===-->
    <link href="/static/assets_user/css/plugins/magnific-popup.css" rel="stylesheet">
    <!--=== Owl Carousel CSS ===-->
    <link href="/static/assets_user/css/plugins/owl.carousel.min.css" rel="stylesheet">
    <!--=== Gijgo CSS ===-->
    <link href="/static/assets_user/css/plugins/gijgo.css" rel="stylesheet">
    <!--=== FontAwesome CSS ===-->
    <link href="/static/assets_user/css/font-awesome.css" rel="stylesheet">
    <!--=== Theme Reset CSS ===-->
    <link href="/static/assets_user/css/reset.css" rel="stylesheet">
    <!--=== Main Style CSS ===-->
    <link href="/static/assets_user/css/style.css" rel="stylesheet">
    <!--=== Responsive CSS ===-->
    <link href="/static/assets_user/css/responsive.css" rel="stylesheet">
</head>

<body class="loader-active">
    <!--== Header Area Start ==-->
    <header id="header-area" class="fixed-top">

        <!--== Header Bottom Start ==-->
        <div id="header-bottom">
            <div class="container">
                <div class="row">
                    <!--== Logo Start ==-->
                    <div class="col-lg-4">
                        
                            <h3>Sistem Perumahan</h3>
                        
                    </div>
                    <!--== Logo End ==-->

                    <!--== Main Menu Start ==-->
                    <div class="col-lg-8 d-none d-xl-block">
                        <nav class="mainmenu alignright">
                            <% if (user) { %>
                                <ul>
                                    <li><a href="/dashboard">Dashboard</a></li>
                                    <li><a href="/profil">Profil</a></li>
                                    <li><a href="/pembayarandetail">Pembayaran</a></li>
                                    <li><a href="/logout">Welcome <%= user.name %> <span> | Logout</span></a></li>
                                </ul>
                            <% } %>
                        </nav>
                    </div>
                    <!--== Main Menu End ==-->
                </div>
            </div>
        </div>
        <!--== Header Bottom End ==-->
    </header>
    <!--== Header Area End ==-->

    
    <!--== Page Title Area Start ==-->
    <section id="page-title-area" class="section-padding overlay">
        <div class="container">
            <div class="row">
                <!-- Page Title Start -->
                <div class="col-lg-12">
                    <div class="section-title  text-center">
                        <h2>Berita Terkini</h2>
                        <span class="title-line"></span>
                    </div>
                </div>
                <!-- Page Title End -->
            </div>
        </div>
    </section>
    <!--== Page Title Area End ==-->

    <!--== Car List Area Start ==-->
    <section id="car-list-area" class="section-padding">
        <div class="container">
            <div class="row">
                <!-- Car List Content Start -->
                <div class="col-lg-12">
                    <div class="car-list-content">
                        <div class="row">
                            <!-- Single Car Start -->
                            <% for (i = 0; i < berita.length; i++) { %>
                            <div class="col-lg-6 col-md-6">
                                <div class="single-car-wrap">
                                    <img src="<%= berita[i].gambar %>" height="300" width="540 ">
                                    <div class="car-list-info without-bar">
                                        <center>
                                        <h4><%= berita[i].judul %></h4>
                                        <p><%= berita[i].keterangan %></p>
                                        </center>
                                    </div>
                                </div>
                            </div>
                            <% } %>
                            
                            <!-- Single Car End -->
                        </div>
                    </div>
                </div>
                <!-- Car List Content End -->
            </div>
        </div>
    </section>
    <!--== Car List Area End ==-->

    <!--== Footer Area Start ==-->
    <section id="footer-area">

    <!-- Footer Bottom Start -->
    <div class="footer-bottom-area fixed-bottom">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                </div>
            </div>
        </div>
    </div>
    <!-- Footer Bottom End -->
</section>
<!--== Footer Area End ==-->
<!--=======================Javascript============================-->
<!--=== Jquery Min Js ===-->
<script src="/static/assets_user/js/jquery-3.2.1.min.js"></script>
<!--=== Jquery Migrate Min Js ===-->
<script src="/static/assets_user/js/jquery-migrate.min.js"></script>
<!--=== Popper Min Js ===-->
<script src="/static/assets_user/js/popper.min.js"></script>
<!--=== Bootstrap Min Js ===-->
<script src="/static/assets_user/js/bootstrap.min.js"></script>
<!--=== Gijgo Min Js ===-->
<script src="/static/assets_user/js/plugins/gijgo.js"></script>
<!--=== Vegas Min Js ===-->
<script src="/static/assets_user/js/plugins/vegas.min.js"></script>
<!--=== Isotope Min Js ===-->
<script src="/static/assets_user/js/plugins/isotope.min.js"></script>
<!--=== Owl Caousel Min Js ===-->
<script src="/static/assets_user/js/plugins/owl.carousel.min.js"></script>
<!--=== Waypoint Min Js ===-->
<script src="/static/assets_user/js/plugins/waypoints.min.js"></script>
<!--=== CounTotop Min Js ===-->
<script src="/static/assets_user/js/plugins/counterup.min.js"></script>
<!--=== YtPlayer Min Js ===-->
<script src="/static/assets_user/js/plugins/mb.YTPlayer.js"></script>
<!--=== Magnific Popup Min Js ===-->
<script src="/static/assets_user/js/plugins/magnific-popup.min.js"></script>
<!--=== Slicknav Min Js ===-->
<script src="/static/assets_user/js/plugins/slicknav.min.js"></script>

<!--=== Mian Js ===-->
<script src="/static/assets_user/js/main.js"></script>

</body>

</html>